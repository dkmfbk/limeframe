	rdfs:range xsd:string ;
	rdfs:comment "String representing the syntax of the Frame"@en ;
	rdfs:label "frame syntax description"@en .
# 
# http://premon.fbk.eu/ontology/vn#frameXtag

pmovn:frameXtag a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain pmovn:VerbNetFrame ;
	rdfs:range xsd:string ;
	rdfs:label "frame xtag"@en .
# 
# http://premon.fbk.eu/ontology/vn#implPredArg

pmovn:implPredArg a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain pmovn:PredArg ;
	rdfs:range xsd:boolean ;
	rdfs:label "implicit predicate argument"@en .
# 
# http://premon.fbk.eu/ontology/vn#negPred

pmovn:negPred a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain pmovn:Pred ;
	rdfs:range xsd:boolean ;
	rdfs:label "negated predicate"@en .
