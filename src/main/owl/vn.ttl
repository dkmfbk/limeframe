_:genid29 a owl:Class ;
	owl:unionOf _:genid31 .

_:genid31 a rdf:List ;
	rdf:first pmovn:RoleSelectionalRestriction ;
	rdf:rest _:genid30 .

_:genid30 a rdf:List ;
	rdf:first pmovn:SyntacticRestriction ;
	rdf:rest rdf:nil .

pmovn:NpSynItem rdfs:subClassOf _:genid32 .

_:genid32 a owl:Restriction ;
	owl:onProperty pmo:valueObj ;
