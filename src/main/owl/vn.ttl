# #    Object Properties
# #
# #################################################################
# 
# 
# http://premon.fbk.eu/ontology/vn#definesFrame

pmovn:definesFrame a owl:ObjectProperty ;
	rdfs:subPropertyOf pmovn:frame ;
	rdfs:label "defines frame"@en .
# 
# http://premon.fbk.eu/ontology/vn#definesSemRole

pmovn:definesSemRole a owl:ObjectProperty ;
	rdfs:subPropertyOf pmo:semRole ;
	rdfs:domain pmovn:VerbClass ;
	rdfs:range pmovn:SemanticRole ;
	rdfs:label "defines semantic role"@en .
# 
# http://premon.fbk.eu/ontology/vn#frame

pmovn:frame a owl:ObjectProperty ;
	rdfs:domain pmovn:VerbClass ;
	rdfs:range pmovn:VerbNetFrame ;
	rdfs:label "frame"@en .
# 
# http://premon.fbk.eu/ontology/vn#frameExample

pmovn:frameExample a owl:ObjectProperty , owl:FunctionalProperty ;
	rdfs:comment "directly associates a VerbNet frame to its corresponding example"@en ;
	rdfs:label "frame example"@en .
# 
# http://premon.fbk.eu/ontology/vn#restriction

pmovn:restriction a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:topObjectProperty ;
	rdfs:domain _:genid1 .

_:genid1 a owl:Class ;
	owl:unionOf _:genid4 .

_:genid4 a rdf:List ;
	rdf:first pmovn:NpSynItem ;
	rdf:rest _:genid3 .

_:genid3 a rdf:List ;
	rdf:first pmovn:PrepSynItem ;
	rdf:rest _:genid2 .

_:genid2 a rdf:List ;
	rdf:first pmovn:SemanticRole ;
	rdf:rest rdf:nil .

pmovn:restriction rdfs:range pmovn:Restriction ;
	rdfs:label "restriction"@en .
# 
# http://premon.fbk.eu/ontology/vn#subclassOf

pmovn:subclassOf a owl:ObjectProperty ;
	rdfs:subPropertyOf pmo:classRel , skos:broader ;
	rdfs:domain pmovn:VerbClass ;
	rdfs:range pmovn:VerbClass ;
	rdfs:label "subclass of"@en .
# 
# http://premon.fbk.eu/ontology/vn#thematicRole

pmovn:thematicRole a owl:ObjectProperty , owl:FunctionalProperty ;
	rdfs:domain pmovn:SemanticRole ;
	rdfs:range pmovn:ThematicRole ;
	rdfs:label "thematic role"@en .
# 
# http://www.lexinfo.net/ontology/2.0/lexinfo#partOfSpeech

<http://www.lexinfo.net/ontology/2.0/lexinfo#partOfSpeech> a owl:ObjectProperty .
