	rdfs:label "And Compound Restriction"@en .
# 
# http://premon.fbk.eu/ontology/vn#AtomicRestriction

pmovn:AtomicRestriction a owl:Class ;
	rdfs:subClassOf pmovn:Restriction , _:genid5 .

_:genid5 a owl:Restriction ;
	owl:onProperty pmo:valueObj ;
	owl:cardinality "1"^^xsd:nonNegativeInteger .

pmovn:AtomicRestriction owl:disjointUnionOf _:genid7 .

_:genid7 a rdf:List ;
	rdf:first pmovn:AbsentAtomicRestriction ;
	rdf:rest _:genid6 .

_:genid6 a rdf:List ;
	rdf:first pmovn:ExistAtomicRestriction ;
	rdf:rest rdf:nil .

pmovn:AtomicRestriction rdfs:label "Atomic Restriction"@en .
# 
# http://premon.fbk.eu/ontology/vn#AuxnpType

pmovn:AuxnpType a owl:Class ;
	owl:equivalentClass _:genid8 .

_:genid8 a owl:Class ;
	owl:oneOf _:genid12 .

_:genid12 a rdf:List ;
	rdf:first pmovn:np_auxnpType ;
	rdf:rest _:genid11 .

_:genid11 a rdf:List ;
	rdf:first pmovn:oblique1_auxnpType ;
	rdf:rest _:genid10 .

_:genid10 a rdf:List ;
	rdf:first pmovn:oblique2_auxnpType ;
	rdf:rest _:genid9 .

_:genid9 a rdf:List ;
	rdf:first pmovn:oblique_auxnpType ;
	rdf:rest rdf:nil .

pmovn:AuxnpType rdfs:label "Auxiliary NP Type"@en .
# 
# http://premon.fbk.eu/ontology/vn#CompoundRestriction

pmovn:CompoundRestriction a owl:Class ;
	rdfs:subClassOf pmovn:Restriction , _:genid13 .

_:genid13 a owl:Restriction ;
	owl:onProperty pmo:item ;
	owl:someValuesFrom owl:Thing .

pmovn:CompoundRestriction owl:disjointUnionOf _:genid15 .

_:genid15 a rdf:List ;
	rdf:first pmovn:AndCompoundRestriction ;
	rdf:rest _:genid14 .

_:genid14 a rdf:List ;
	rdf:first pmovn:OrCompoundRestriction ;
	rdf:rest rdf:nil .

pmovn:CompoundRestriction rdfs:label "Compound Restriction"@en .
# 
# http://premon.fbk.eu/ontology/vn#ConstantPredArg

pmovn:ConstantPredArg a owl:Class ;
	rdfs:subClassOf pmovn:PredArg ;
