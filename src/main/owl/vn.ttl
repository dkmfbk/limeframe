pmovn:EventPredArg rdfs:subClassOf _:genid17 .

_:genid17 a owl:Restriction ;
	owl:onProperty pmo:valueObj ;
	owl:cardinality "1"^^xsd:nonNegativeInteger .

pmovn:EventPredArg rdfs:label "Event Predicate Argument"@en .
# 
# http://premon.fbk.eu/ontology/vn#EventPredArgType

pmovn:EventPredArgType a owl:Class ;
	owl:equivalentClass _:genid18 .

_:genid18 a owl:Class ;
	owl:oneOf _:genid22 .

_:genid22 a rdf:List ;
	rdf:first pmovn:duringEventArg ;
	rdf:rest _:genid21 .

_:genid21 a rdf:List ;
	rdf:first pmovn:endEventArg ;
	rdf:rest _:genid20 .

_:genid20 a rdf:List ;
	rdf:first pmovn:resultEventArg ;
	rdf:rest _:genid19 .

_:genid19 a rdf:List ;
	rdf:first pmovn:startEventArg ;
	rdf:rest rdf:nil .

pmovn:EventPredArgType rdfs:label "Event Predicate Argument Type"@en .
# 
# http://premon.fbk.eu/ontology/vn#ExistAtomicRestriction

pmovn:ExistAtomicRestriction a owl:Class ;
	rdfs:subClassOf pmovn:AtomicRestriction ;
	rdfs:label "Exist Atomic Restriction"@en .
# 
# http://premon.fbk.eu/ontology/vn#LexSynItem

pmovn:LexSynItem a owl:Class ;
